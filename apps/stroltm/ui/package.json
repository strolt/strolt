{
  "name": "stroltm-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gen-api": "yarn run openapi-generator-cli generate -i ../../../.swagger/stroltm/swagger.json -g typescript-axios -o ./src/api/generated",
    "format": "prettier --plugin @trivago/prettier-plugin-sort-imports --write",
    "format:all": "prettier --plugin @trivago/prettier-plugin-sort-imports --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "tsc"
  },
  "dependencies": {
    "antd": "^5.0.5",
    "axios": "^1.1.3",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "mobx-utils": "^6.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router": "^6.4.5",
    "react-router-dom": "^6.4.5"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^4.0.1"
  }
}
