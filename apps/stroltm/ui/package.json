{
  "name": "stroltm-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gen-api": "yarn run openapi-generator-cli generate -i ../../../.swagger/stroltm/swagger.json -g typescript-axios -o ./src/api/generated",
    "format": "prettier --plugin @trivago/prettier-plugin-sort-imports --write",
    "format:all": "prettier --plugin @trivago/prettier-plugin-sort-imports --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "tsc"
  },
  "dependencies": {
    "antd": "^5.4.0",
    "axios": "^1.3.4",
    "cronstrue": "^2.26.0",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "mobx-utils": "^6.0.6",
    "pretty-ms": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "typescript": "5.0.3",
    "vite": "^4.2.3",
    "vite-tsconfig-paths": "^4.0.8"
  }
}
