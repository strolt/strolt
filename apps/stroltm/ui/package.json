{
  "name": "stroltm-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gen-api": "pnpm exec openapi-generator-cli generate -i ../../../.swagger/stroltm/swagger.json -g typescript-axios -o ./src/api/generated",
    "format": "prettier --plugin @trivago/prettier-plugin-sort-imports --write",
    "format:all": "prettier --plugin @trivago/prettier-plugin-sort-imports --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "6.1.0",
    "antd": "6.0.1",
    "axios": "1.13.2",
    "cronstrue": "3.9.0",
    "mobx": "6.15.0",
    "mobx-react-lite": "4.1.1",
    "mobx-utils": "6.1.1",
    "pretty-ms": "9.3.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-json-view": "1.21.3",
    "react-router": "7.10.0",
    "react-router-dom": "7.10.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.25.2",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vanilla-extract/css": "1.17.4",
    "@vanilla-extract/vite-plugin": "5.1.3",
    "@vitejs/plugin-react-swc": "4.2.2",
    "prettier": "3.7.3",
    "typescript": "5.9.3",
    "vite": "7.2.6",
    "vite-tsconfig-paths": "5.1.4"
  }
}
